{
  "version": 2,
  "builds": [
    {
      "src": "PriceStore/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "buildCommand": "python3 manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "PriceStore/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles"
}
